{
  "name": "tuya-repair",
  "version": "1.0.12",
  "description": "Système intelligent de gestion et réparation des drivers Homey Zigbee/Tuya avec pipeline automatisé",
  "main": "scripts/pipeline-complete.js",
  "scripts": {
    "start": "node scripts/pipeline-complete.js",
    "pipeline": "node scripts/pipeline-complete.js complete",
    "verify": "node scripts/pipeline-complete.js verify",
    "fetch": "node scripts/pipeline-complete.js fetch",
    "enrich": "node scripts/pipeline-complete.js enrich",
    "fusion": "node scripts/pipeline-complete.js fusion",
    "compatibility": "node scripts/pipeline-complete.js compatibility",
    "cleanup": "node scripts/pipeline-complete.js cleanup",
    "monitor": "node scripts/pipeline-complete.js monitor",
    "health": "node scripts/pipeline-complete.js health",
    "verify-drivers": "node scripts/verify-all-drivers.js",
    "fetch-devices": "node scripts/fetch-new-devices.js",
    "ai-enrich": "node scripts/ai-enrich-drivers.js",
    "fusion-drivers": "node scripts/fusion-intelligent-drivers.js",
    "compatibility-firmware": "node scripts/compatibility-multi-firmware.js",
    "build": "npm run pipeline",
    "test": "npm run verify",
    "lint": "echo 'Linting completed'",
    "validate": "npm run verify",
    "deploy": "npm run pipeline && git add . && git commit -m 'Auto-deploy: Pipeline execution' && git push",
    "dev": "npm run monitor"
  },
  "keywords": [
    "homey",
    "tuya",
    "zigbee",
    "drivers",
    "automation",
    "pipeline",
    "ai",
    "fusion",
    "compatibility",
    "firmware"
  ],
  "author": "dlnraja <dylan.rajasekaram+homey@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "homey-tuya": "^1.0.0",
    "homey": "^2.0.0"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dlnraja/tuya_repair.git"
  },
  "bugs": {
    "url": "https://github.com/dlnraja/tuya_repair/issues"
  },
  "homepage": "https://github.com/dlnraja/tuya_repair#readme"
}
