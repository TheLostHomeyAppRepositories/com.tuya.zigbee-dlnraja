{
  "name": "tuya-zigbee-temp",
  "version": "1.0.0",
  "description": "Configuration temporaire pour ex√©cution des scripts",
  "type": "module",
  "private": true,
  "dependencies": {
    "axios": "1.6.2",
    "chalk": "5.3.0",
    "fs-extra": "11.1.1",
    "glob": "10.3.3",
    "uuid": "9.0.0"
  },
  "scripts": {
    "live-update": "node scripts/live-update.js",
    "fix-js": "node scripts/fix-js.js",
    "start": "homey app run",
    "dev": "cross-env NODE_ENV=development homey app run",
    "build": "npm run process-assets && homey app build",
    "process-assets": "node tools/process-assets.js",
    "validate": "npm run lint && npm run test && npm run process-assets && homey app validate --level=publish",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:coverage",
    "generate:driver": "node generate-driver.js",
    "orchestrate": "node orchestrate.js",
    "test:unit": "mocha --config .mocharc.new.json",
    "test:integration": "mocha test/integration/**/*.test.js --timeout 30000 --exit",
    "test:e2e": "mocha test/e2e/**/*.test.js --timeout 60000 --exit",
    "test:watch": "mocha --config .mocharc.new.json --watch",
    "test:coverage": "nyc --reporter=text --reporter=lcov --reporter=html --reporter=cobertura --reporter=text-summary --reporter=json-summary mocha --config .mocharc.new.json && node tools/generate-coverage-report.js",
    "scan:full": "node scripts/windsurf-scan.js",
    "scan:scout": "node scripts/scout.js",
    "scan:architect": "node scripts/architect.js",
    "scan:localizer": "node scripts/localizer.js",
    "scan:optimizer": "node scripts/optimizer.js",
    "scan:validator": "node scripts/validator.js",
    "test:debug": "cross-env NODE_DEBUG=* mocha --config .mocharc.new.json --inspect-brk",
    "lint": "eslint . --ext .js --cache --cache-location ./node_modules/.cache/eslint",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md,yml,yaml}\"",
    "prettier": "prettier --check \"**/*.{js,json,md,yml,yaml}\"",
    "validate:drivers": "node tools/validate-driver-ids.js",
    "validate:app-json": "node tools/validate-app-json.js",
    "validate:all": "npm run validate:zigbee && npm run validate:app-json && npm run validate:drivers && npm run validate:configs",
    "validate:zigbee": "node tools/validate-zigbee-properties.js",
    "validate:configs": "node tools/validate-driver-configs.js",
    "python:start": "cd python_service && python -m uvicorn main:app --reload --port 8000",
    "python:test": "cd python_service && python -m pytest -v",
    "python:coverage": "cd python_service && python -m pytest --cov=. --cov-report=term-missing",
    "python:install": "cd python_service && pip install -r requirements.txt",
    "docker:build": "docker build -t tuya-zigbee .",
    "docker:run": "docker run -p 3000:3000 -p 8000:8000 tuya-zigbee",
    "dev:docker": "docker-compose up --build",
    "clean": "rm -rf node_modules package-lock.json .homey* .nyc_output coverage",
    "postinstall": "homey app install && npm run check-updates",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "prepush": "npm run validate",
    "check-updates": "npx npm-check-updates -u",
    "update": "ncu -u && npm install",
    "coverage": "open coverage/lcov-report/index.html",
    "docs": "jsdoc -c .jsdoc.json",
    "analyze": "source-map-explorer 'dist/**/*.js'",
    "integrate": "node scripts/integrate.js",
    "docs:generate": "node scripts/auto-documenter.js",
    "icons:generate": "node scripts/generate-icons.js",
    "docs:update": "npm run docs:generate && npm run icons:generate",
    "ci:integrate": "node scripts/integrate.js --no-git"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@homey/sdk": "^3.6.0",
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "homey": "^3.6.0",
    "homey-log": "^2.1.0",
    "homey-meshdriver": "^1.4.0",
    "homey-tuya": "github:dlnraja/homey-tuya#main",
    "joi": "^17.7.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "p-queue": "^7.3.4",
    "p-retry": "^5.1.1",
    "sharp": "^0.32.6",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
