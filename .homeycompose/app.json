  "name": {
    "en": "Ultimate Zigbee Hub",
    "fr": "Ultimate Zigbee Hub",
    "nl": "Ultimate Zigbee Hub", 
    "de": "Ultimate Zigbee Hub"
  },
  "description": {
    "en": "Professional Zigbee support for 800+ Tuya devices. Clean SDK3 architecture with comprehensive device categories. Local communication only - no cloud dependencies!",
    "fr": "Support Zigbee professionnel pour 800+ appareils Tuya. Architecture SDK3 propre avec catégories complètes. Communication locale uniquement - aucune dépendance cloud!",
    "nl": "Professionele Zigbee ondersteuning voor 800+ Tuya apparaten. Schone SDK3 architectuur met uitgebreide apparaatcategorieën. Alleen lokale communicatie - geen cloud afhankelijkheden!",
    "de": "Professionelle Zigbee Unterstützung für 800+ Tuya Geräte. Saubere SDK3 Architektur mit umfassenden Gerätekategorien. Nur lokale Kommunikation - keine Cloud Abhängigkeiten!"
  },
  "category": ["lights", "appliances"],
  "brandColor": "#FF6900",
  "permissions": [],
  "images": {
    "small": "/assets/images/small.png",
    "large": "/assets/images/large.png",
    "xlarge": "/assets/images/xlarge.png"
  },
  "author": {
    "name": "dlnraja",
    "email": "support@community.tuya.zigbee"
  },
  "bugs": {
    "url": "https://community.homey.app/t/app-pro-universal-tuya-zigbee-device-app-lite-version/140352"
  },
  "support": "https://community.homey.app/t/app-pro-universal-tuya-zigbee-device-app-lite-version/140352",
  "homeyCommunityTopicId": 140352,
  "flow": {
    "triggers": [
      {
        "id": "motion_detected",
        "title": {
          "en": "Motion detected",
          "fr": "Mouvement détecté",
          "nl": "Beweging gedetecteerd",
          "de": "Bewegung erkannt"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=tuya_motion_sensor"
          }
        ]
      },
      {
        "id": "contact_opened",
        "title": {
          "en": "Contact opened",
          "fr": "Contact ouvert",
          "nl": "Contact geopend",
          "de": "Kontakt geöffnet"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=tuya_door_window_sensor"
          }
        ]
      },
      {
        "id": "water_leak_detected",
        "title": {
          "en": "Water leak detected",
          "fr": "Fuite d'eau détectée",
          "nl": "Waterlek gedetecteerd",
          "de": "Wasserleck erkannt"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=tuya_water_leak_sensor"
          }
        ]
      }
    ],
    "conditions": [
      {
        "id": "is_motion_active",
        "title": {
          "en": "Motion is active",
          "fr": "Le mouvement est actif",
          "nl": "Beweging is actief",
          "de": "Bewegung ist aktiv"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=tuya_motion_sensor"
          }
        ]
      }
    ],
    "actions": [
      {
        "id": "turn_on_light",
        "title": {
          "en": "Turn light on",
          "fr": "Allumer la lumière",
          "nl": "Licht aanzetten",
          "de": "Licht anschalten"
        },
        "args": [
          {
            "name": "device",
            "type": "device",
            "filter": "driver_id=tuya_smart_light|tuya_light_switch"
          }
        ]
      }
    ]
  },
